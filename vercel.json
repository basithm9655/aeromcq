{
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Cache-Control",
                    "value": "no-store"
                }
            ]
        },
        {
            "source": "/questions.json",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600"
                }
            ]
        },
        {
            "source": "/logo.png",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400, immutable"
                }
            ]
        }
    ]
}
